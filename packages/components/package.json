  "scripts": {
    "start": "npm run storybook",
    "build": "npm-run-all clean build:lit build:custom-elements-manifest build:storybook",
    "build:lit": "rollup -c",
    "build:custom-elements-manifest": "npx custom-elements-manifest analyze --litelement --globs \"src/**/*.ts\" --exclude \"./**/*.stories.ts\" --exclude \"./storybook-static\"",
    "build:storybook": "build-storybook --webpack-stats-json",
    "storybook": "npm-run-all build:custom-elements-manifest -p storybook:start",
    "storybook:start": "start-storybook -p 6006",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "lint": "npm-run-all lint:*",
    "lint:eslint": "eslint src",
    "lint:lit": "lit-analyzer",
    "test": "npm-run-all test:*",
    "test:lit": "web-test-runner --node-resolve --coverage",
    "watch": "npm-run-all -p watch:*",
    "watch:rollup": "rollup -c -w",
    "watch:test": "web-test-runner --node-resolve --coverage --watch",
    "clean": "npm-run-all clean:*",
    "clean:dist": "rimraf dist",
    "clean:storybook": "rimraf storybook-static",
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --cache --fix",
      "lit-analyzer --"
    ],
    "*.{js,ts,tsx,css,md,mdx,yml,json,mjs,pcss}": "prettier --write"
  },
  "publishConfig": {
    "directory": "dist"
  },
  "customElements": "custom-elements.json",
